    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/Assets/client/lib/wow/wow.min.js"></script>
    <script src="~/Assets/client/lib/easing/easing.min.js"></script>
